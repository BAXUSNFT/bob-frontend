{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/chat/:agentId",
      "destination": "/index.html"
    },
    {
      "source": "/chat/:agentId/:roomId",
      "destination": "/index.html"
    }
  ]
}